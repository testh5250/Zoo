<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="720.0" prefWidth="1024.0" styleClass="loginBackground" stylesheets="@Theme.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="H5250_project.controller.SignUpFormController">
    <children>
        <Button layoutX="32.0" layoutY="33.0" mnemonicParsing="false" onAction="#handleBack" prefHeight="41.0" prefWidth="41.0" styleClass="backButton" stylesheets="@Theme.css" />
      <VBox layoutX="379.0" layoutY="245.0" prefHeight="369.0" prefWidth="266.0">
         <children>
            <TextField fx:id="usrField" promptText="New UserName" />
            <PasswordField fx:id="passField" promptText="Password" />
            <PasswordField fx:id="confirm_passField" promptText="Confirm Password" />
            <TextField fx:id="emailField" promptText="Email: e.g:user@domain.com" />
            <Button fx:id="signUpBtn" mnemonicParsing="false" onAction="#handleSignUp" prefHeight="28.0" prefWidth="273.0" text="Sign Up" />
         </children>
      </VBox>
    </children>
</AnchorPane>
